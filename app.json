{
  "expo": {
    "name": "PokeBeer",
    "slug": "PokeBeer",
    "description": "A drink tracking application to rate your favorite and not so favorite drinks ",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#7f70e7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "app/assets/*"
    ],
    "android": {
      "package": "com.moshea.pokebeer"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moshea.pokebeer"
    }
  }
}
